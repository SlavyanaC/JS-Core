{{#each conta}}
    <div class="contact card">
        <div>
            <i class="far fa-user-circle gravatar"></i>
        </div>
    </div>
    <div class="info">
        <h2>Name: {{name}}</h2>
        <button class="detailsBtn" onclick="showDetails({{id}})"></button>
        <div class="details" id="{{id}}">
            <p>Phone number: {{phoneNumber}}</p>
            <p>Email: {{eimal}}</p>
        </div>
    </div>
{{/each}}